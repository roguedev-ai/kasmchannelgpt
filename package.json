{
  "name": "customgpt-ui",
  "version": "1.0.0",
  "description": "CustomGPT.ai Chat UI - Multi-format deployment (standalone, widget, embed)",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:https": "HTTPS=true SSL_CRT_FILE=localhost.pem SSL_KEY_FILE=localhost-key.pem next dev --experimental-https",
    "build": "next build",
    "build:standalone": "next build",
    "build:widget": "webpack --config webpack.widget.js --mode production && node scripts/copy-widget-assets.js",
    "build:iframe": "webpack --config webpack.iframe.js --mode production && node scripts/copy-widget-assets.js",
    "dev:widget": "webpack serve --config webpack.widget.js --mode development",
    "dev:iframe": "webpack serve --config webpack.iframe.js --mode development",
    "build:all": "npm run build:standalone && npm run build:widget && npm run build:iframe",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@types/dompurify": "^3.0.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "dompurify": "^3.0.7",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.303.0",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-icons": "^5.0.0",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "recharts": "^3.1.0",
    "remark-gfm": "^4.0.0",
    "sonner": "^1.3.1",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7",
    "openai": "^4.26.0",
    "@ricky0123/vad-react": "^0.0.18",
    "react-spinners": "^0.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^9.2.1",
    "css-loader": "^6.11.0",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.5.6",
    "postcss-loader": "^7.3.4",
    "style-loader": "^4.0.0",
