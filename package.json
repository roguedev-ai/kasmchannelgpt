{
  "name": "kasmchannelgpt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate": "ts-node scripts/migrate-collections.ts",
    "logs": "tail -f logs/app.log",
    "health-check": "curl -f http://localhost:3000/api/health || exit 1",
    "backup": "mkdir -p backups && cp data/app.db \"backups/app.db.$(date +%Y%m%d_%H%M%S)\"",
    "restore": "bash -c 'if [ -z \"$1\" ]; then echo \"Usage: npm run restore <backup-file>\"; exit 1; fi; cp \"$1\" data/app.db'",
    "list-backups": "ls -l backups/",
    "clean": "rm -rf .next out",
    "deploy": "npm run build && npm run migrate && npm start"
  },
  "dependencies": {
    "@auth/core": "latest",
    "@tanstack/react-query": "latest",
    "@tanstack/react-table": "latest",
    "bcryptjs": "latest",
    "better-sqlite3": "latest",
    "drizzle-orm": "latest",
    "nanoid": "latest",
    "next": "latest",
    "next-auth": "latest",
    "react": "latest",
    "react-dom": "latest",
    "sonner": "latest"
  },
  "devDependencies": {
    "@types/bcryptjs": "latest",
    "@types/better-sqlite3": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "autoprefixer": "latest",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "postcss": "latest",
    "tailwindcss": "latest",
    "ts-node": "latest",
    "typescript": "latest"
  }
}
